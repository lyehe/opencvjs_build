{
  "name": "opencv-contrib-wasm",
  "version": "4.13.0",
  "description": "Precompiled OpenCV 4.13.0 with all Contrib modules to JavaScript + WebAssembly for Node.js, Deno, and browsers. Includes SIMD optimization and multi-threading support. Available in Essential (~3MB) and Full (~12MB) builds.",
  "main": "src/index.js",
  "module": "src/index.mjs",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./src/index.mjs",
      "require": "./src/index.js"
    },
    "./essential": {
      "types": "./types/index.d.ts",
      "import": "./src/essential.mjs",
      "require": "./src/essential.js"
    },
    "./full": {
      "types": "./types/index.d.ts",
      "import": "./src/full.mjs",
      "require": "./src/full.js"
    },
    "./dist/essential/*": "./dist/essential/*",
    "./dist/full/*": "./dist/full/*"
  },
  "files": [
    "src/",
    "dist/",
    "types/",
    "examples/"
  ],
  "scripts": {
    "download": "bash scripts/download-opencv.sh",
    "build": "npm run build:essential && npm run build:full",
    "build:essential": "BUILD_TYPE=essential bash scripts/build.sh",
    "build:full": "BUILD_TYPE=full bash scripts/build.sh",
    "build:docker": "docker build -t opencv-wasm-builder . && docker run --rm -v \"$(pwd)\":/src opencv-wasm-builder",
    "clean": "rm -rf opencv opencv_contrib build_essential build_full dist/essential dist/full",
    "test": "node examples/node/basic.js && node examples/node/image-processing.js && node examples/node/feature-detection.js && node examples/node/aruco-detection.js && node examples/node/contours.js",
    "generate-markers": "node examples/node/generate-markers.js",
    "prepublishOnly": "echo 'Ready to publish'"
  },
  "keywords": [
    "opencv",
    "opencv.js",
    "opencvjs",
    "wasm",
    "webassembly",
    "computer-vision",
    "image-processing",
    "machine-learning",
    "deep-learning",
    "contrib",
    "aruco",
    "face-recognition",
    "feature-detection",
    "orb",
    "sift",
    "surf",
    "emscripten",
    "simd",
    "threading",
    "browser",
    "nodejs",
    "deno"
  ],
  "author": "",
  "license": "Unlicense",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyehe/opencvjs_build.git"
  },
  "bugs": {
    "url": "https://github.com/lyehe/opencvjs_build/issues"
  },
  "homepage": "https://github.com/lyehe/opencvjs_build#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/lyehe"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64",
    "ia32"
  ],
  "sideEffects": false,
  "devDependencies": {}
}
